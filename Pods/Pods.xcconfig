CLANG_CXX_LIBRARY = compiler-default
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/OpenCV" "$(PODS_ROOT)/TesseractOCRiOS/Products"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/MBProgressHUD" "${PODS_ROOT}/Headers/MagicalRecord" "${PODS_ROOT}/Headers/OpenCV" "${PODS_ROOT}/Headers/REFrostedViewController" "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/MagicalRecord" -isystem "${PODS_ROOT}/Headers/OpenCV" -isystem "${PODS_ROOT}/Headers/REFrostedViewController" -isystem "${PODS_ROOT}/Headers/TesseractOCRiOS"
OTHER_LDFLAGS = -ObjC  -weak_library /usr/lib/libstdc++.6.0.9.dylib -lstdc++ -framework AVFoundation -framework Accelerate -framework AssetsLibrary -framework CoreData -framework CoreGraphics -framework CoreImage -framework CoreMedia -framework CoreVideo -framework Foundation -framework QuartzCore -framework TesseractOCR -framework UIKit -framework opencv2
PODS_ROOT = ${SRCROOT}/Pods